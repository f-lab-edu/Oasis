<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flab.oasis.mapper.user.ConstantDefinitionMapper">

    <select id="getIntValueByConstantName" parameterType="com.flab.oasis.constant.ConstantName" resultType="java.lang.Integer">
        SELECT INT_VALUE
        FROM CONSTANT_DEFINITION
        WHERE CONSTANT_NAME = #{constantName}
    </select>

</mapper>